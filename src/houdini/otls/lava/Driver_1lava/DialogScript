            parmtag { "filechooser_mode" "write" }
            parmtag { "script_callback_language" "python" }
        }
        parm {
            name    "lv_tmplocalstorage"
            label   "Local temp storage"
            type    file
            default { "$HOUDINI_TEMP_DIR/lsds/storage" }
            disablewhen "{ lv_inlinestorage != 0 }"
            parmtag { "script_callback_language" "python" }
        }
        parm {
            name    "soho_viewport_menu"
            label   "Show In Viewport Menu"
            type    toggle
            default { "1" }
        }
        parm {
            name    "soho_force_compression"
            label   "SOHO Force Compression"
            type    toggle
            invisible
            default { "1" }
            parmtag { "script_callback_language" "python" }
        }
    }

