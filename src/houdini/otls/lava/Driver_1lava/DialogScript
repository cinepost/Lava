        }
        parm {
            name    "lv_binarygeometry"
            label   "Save Binary Geometry"
            type    toggle
            default { "1" }
            parmtag { "spare_category" "Driver" }
        }
        parm {
            name    "soho_compression"
            label   "File Compression"
            type    string
            default { "ext" }
