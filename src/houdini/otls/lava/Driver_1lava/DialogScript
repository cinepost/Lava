                    help    "Use screen space computed ambient occlusion."
                    parmtag { "script_callback_language" "python" }
                }
                parm {
                    name    "lv_ambient_occlusion_distance"
                    label   "Distance"
                    type    float
                    default { "0.5" }
                    disablewhen "{ lv_ambient_occlusion == 0 }"
                    range   { 0 5 }
                    parmtag { "script_callback_language" "python" }
                }
                parm {
                    name    "lv_ambient_occlusion_factor"
                    label   "Factor"
                    type    float
                    default { "1" }
                    disablewhen "{ lv_ambient_occlusion == 0 }"
                    range   { 0! 1 }
                    parmtag { "script_callback_language" "python" }
                }
                parm {
                    name    "lv_ambient_occlusion_precision"
                    label   "Precision"
                    type    float
                    default { "1" }
                    disablewhen "{ lv_ambient_occlusion == 0 }"
                    range   { 0! 1 }
                    parmtag { "script_callback_language" "python" }
                }
                parm {
                    name    "lv_ambient_occlusion_bent_normals"
                    label   "Bent Normals"
                    type    toggle
                    default { "0" }
                    disablewhen "{ lv_ambient_occlusion == 0 }"
                    parmtag { "script_callback_language" "python" }
                }
                parm {
                    name    "lv_ambient_occlusion_bounce_approx"
                    label   "Bounce Approximation"
                    type    toggle
                    default { "0" }
                    disablewhen "{ lv_ambient_occlusion == 0 }"
                    parmtag { "script_callback_language" "python" }
                }
            }
        }
    }

    group {
        name    "images_2"
        label   "Objects"

        parm {
            name    "vobject"
            label   "Candidate Objects"
            type    oplist
            default { "*" }
            parmtag { "opfilter" "!!OBJ/GEOMETRY!!" }
            parmtag { "oprelative" "/" }
        }
        parm {
            name    "alights"
            label   "Candidate Lights"
            type    oplist
            default { "*" }
            disablewhen "{ sololight != \"\" }"
            parmtag { "opfilter" "!!OBJ/LIGHT!!" }
            parmtag { "oprelative" "/obj" }
            parmtag { "script_callback_language" "hscript" }
        }
        parm {
            name    "sepparm"
            label   "Separator"
            type    separator
            default { "" }
        }
        parm {
            name    "sololight"
            label   "Solo Light"
            type    oplist
            default { "" }
            help    "Solo Lights"
            parmtag { "opfilter" "!!OBJ/LIGHT!!" }
            parmtag { "oprelative" "/obj" }
        }
        parm {
            name    "soho_autoheadlight"
            label   "Headlight Creation"
            type    toggle
            default { "1" }
            disablewhen "{ sololight != \"\" }"
        }
    }

    group {
        name    "images_3"
        label   "Driver"

        parm {
